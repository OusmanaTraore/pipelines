node('slavejenkins') {
    
    stage('Git') {
        // Get some code from a GitHub repository
        git url: 'https://gitlab.com/projetcd/devopsapp.git'
    }

    withMaven(
        // Maven installation declared in the Jenkins "Global Tool Configuration"
        maven: 'maven') {

      // Run the maven build
      sh "mvn clean package"
    }
}
